{% extends 'base.html.twig' %}

{% block title %}Articles{% endblock %}


{% block body %}

{{ form_start(form) }}
    <div>
        {{ form_errors(form) }}
    </div>

        <div class="form-group">
            {{ form_row(form.title) }}
        </div>
        <div class="form-group" >
            {{ form_row(form.description) }}
        </div>
        <div class="col" >
            {{ form_row(form.type) }}
        </div>
          <div class="col" >
            {{ form_row(form.pictureUrl) }}
        </div>
          <div class="form-group" >
            {{ form_row(form.submit) }}
        </div>
{{ form_end(form) }}


{% endblock %}

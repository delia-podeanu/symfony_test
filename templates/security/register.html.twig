{% extends 'base.html.twig' %}

{% block title %}Register{% endblock %}

{% block body %}

  {{ form_start(form) }}
  <div>
    {{ form_errors(form) }}
  </div>

  <div class="form-group">
    {{ form_row(form.email) }}
  </div>
  <div class="form-group">
    {{ form_row(form.password) }}
  </div>
  <div class="form-group">
    {{ form_row(form.submit) }}
  </div>
  {{ form_end(form) }}

{% endblock %}
